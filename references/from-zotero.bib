	author = {Hamelin, David J. and Fournelle, Dominique and Grenier, Jean-Christophe and Schockaert, Jana and Kovalchik, Kevin A. and Kubiniok, Peter and Mostefai, Fatima and Duquette, Jérôme D. and Saab, Frederic and Sirois, Isabelle and Smith, Martin A. and Pattijn, Sofie and Soudeyns, Hugo and Decaluwe, Hélène and Hussin, Julie and Caron, Etienne},
	month = feb,
	year = {2022},
	pages = {143--157.e3},
	file = {Hamelin et al. - 2022 - The mutational landscape of SARS-CoV-2 variants di.pdf:/home/schagas/snap/zotero-snap/common/Zotero/storage/GKQZPK9I/Hamelin et al. - 2022 - The mutational landscape of SARS-CoV-2 variants di.pdf:application/pdf},
