	author = {Secolin, Rodrigo and de Araujo, Tânia K. and Gonsales, Marina C. and Rocha, Cristiane S. and Naslavsky, Michel and Marco, Luiz De and Bicalho, Maria A. C. and Vazquez, Vinicius L. and Zatz, Mayana and Silva, Wilson A. and Lopes-Cendes, Iscia},
	month = dec,
