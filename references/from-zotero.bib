@article{tavasolian_hla_2021-1,
	title = {{HLA}, {Immune} {Response}, and {Susceptibility} to {COVID}-19},
	volume = {11},
	issn = {1664-3224},
	url = {https://www.frontiersin.org/articles/10.3389/fimmu.2020.601886/full},
	doi = {10.3389/fimmu.2020.601886},
	abstract = {The severe acute respiratory syndrome caused by Coronavirus 2 (SARS-CoV-2) that appeared in December 2019 has precipitated the global pandemic Coronavirus Disease 2019 (COVID-19). However, in many parts of Africa fewer than expected cases of COVID-19, with lower rates of mortality, have been reported. Individual human leukocyte antigen (HLA) alleles can affect both the susceptibility and the severity of viral infections. In the case of COVID-19 such an analysis may contribute to identifying individuals at higher risk of the disease and the epidemiological level to understanding the differences between countries in the epidemic patterns. It is also recognized that first antigen exposure influences the consequence of subsequent exposure. We thus propose a theory incorporating HLA antigens, the “original antigenic sin (OAS)” effect, and presentation of viral peptides which could explain with differential susceptibility or resistance to SARS-CoV-2 infections.},
	language = {en},
	urldate = {2022-06-06},
	journal = {Frontiers in Immunology},
	author = {Tavasolian, Fataneh and Rashidi, Mohsen and Hatam, Gholam Reza and Jeddi, Marjan and Hosseini, Ahmad Zavaran and Mosawi, Sayed Hussain and Abdollahi, Elham and Inman, Robert D.},
	month = jan,
	year = {2021},
	pages = {601886},
	file = {Tavasolian et al. - 2021 - HLA, Immune Response, and Susceptibility to COVID-.pdf:/home/biowitcher/snap/zotero-snap/common/Zotero/storage/GCHTR5M3/Tavasolian et al. - 2021 - HLA, Immune Response, and Susceptibility to COVID-.pdf:application/pdf},
}

@article{migliorini_association_2021,
	title = {Association between {HLA} genotypes and {COVID}-19 susceptibility, severity and progression: a comprehensive review of the literature},
	volume = {26},
	issn = {2047-783X},
	shorttitle = {Association between {HLA} genotypes and {COVID}-19 susceptibility, severity and progression},
	url = {https://eurjmedres.biomedcentral.com/articles/10.1186/s40001-021-00563-1},
	doi = {10.1186/s40001-021-00563-1},
	abstract = {The COVID-19 pandemic has markedly impacted on cultural, political, and economic structures all over the world. Several aspects of its pathogenesis and related clinical consequences have not yet been elucidated. Infection rates, as well morbidity and mortality differed within countries. It is intriguing for scientists to understand how patient genetics may influence the outcome of the condition, to clarify which aspects could be related the clinical variability of SARS-CoV-2 disease. We reviewed the studies exploring the role of human leukocyte antigens (HLA) genotypes on individual responses to SARS-CoV-2 infection and/or progression, discussing also the contribution of the immunological patterns MHC-related. In March 2021, the main online databases were accessed. All the articles that investigated the possible association between the HLA genotypes and related polymorphisms with susceptibility, severity and progression of COVID-19 were considered. Although both genetic and environmental factors are certainly expected to influence the susceptibility to or protection of individuals, the HLA and related polymorphisms can influence susceptibility, progression and severity of SARS-CoV-2 infection. The crucial role played by HLA molecules in the immune response, especially through pathogen-derived peptide presentation, and the huge molecular variability of HLA alleles in the human populations could be responsible for the different rates of infection and the different patients following COVID-19 infection.},
	language = {en},
	number = {1},
	urldate = {2022-06-06},
	journal = {European Journal of Medical Research},
	author = {Migliorini, Filippo and Torsiello, Ernesto and Spiezia, Filippo and Oliva, Francesco and Tingart, Markus and Maffulli, Nicola},
	month = dec,
	year = {2021},
	pages = {84},
	file = {Migliorini et al. - 2021 - Association between HLA genotypes and COVID-19 sus.pdf:/home/biowitcher/snap/zotero-snap/common/Zotero/storage/3YF8Y3NS/Migliorini et al. - 2021 - Association between HLA genotypes and COVID-19 sus.pdf:application/pdf},
}

@article{correale_hla-b44_2020,
	title = {{HLA}-{B}*44 and {C}*01 {Prevalence} {Correlates} with {Covid19} {Spreading} across {Italy}},
	volume = {21},
	issn = {1422-0067},
	url = {https://www.mdpi.com/1422-0067/21/15/5205},
	doi = {10.3390/ijms21155205},
	abstract = {The spread of COVID-19 is showing huge, unexplained, diﬀerences between northern and southern Italy. We hypothesized that the regional prevalence of speciﬁc class I human leukocyte antigen (HLA) alleles, which shape the anti-viral immune response, might partly underlie these diﬀerences. Through an ecological approach, we analyzed whether a set of HLA alleles (A, B, C), known to be involved in the immune response against infections, correlates with COVID-19 incidence. COVID-19 data were provided by the National Civil Protection Department, whereas HLA allele prevalence was retrieved through the Italian Bone-Marrow Donors Registry. Among all the alleles, HLA-A*25, B*08, B*44, B*15:01, B*51, C*01, and C*03 showed a positive log-linear correlation with COVID-19 incidence rate ﬁxed on 9 April 2020 in proximity of the national outbreak peak (Pearson’s coeﬃcients between 0.50 and 0.70, p-value {\textless} 0.0001), whereas HLA-B*14, B*18, and B*49 showed an inverse log-linear correlation (Pearson’s coeﬃcients between −0.47 and −0.59, p-value {\textless} 0.0001). When alleles were examined simultaneously using a multiple regression model to control for confounding factors, HLA-B*44 and C*01 were still positively and independently associated with COVID-19: a growth rate of 16\% (95\%CI: 0.1–35\%) per 1\% point increase in B*44 prevalence; and of 19\% (95\%CI: 1–41\%) per 1\% point increase in C*01 prevalence. Our epidemiologic analysis, despite the limits of the ecological approach, is strongly suggestive of a permissive role of HLA-C*01 and B*44 towards SARS-CoV-2 infection, which warrants further investigation in case-control studies. This study opens a new potential avenue for the identiﬁcation of sub-populations at risk, which could provide Health Services with a tool to deﬁne more targeted clinical management strategies and priorities in vaccination campaigns.},
	language = {en},
	number = {15},
	urldate = {2022-06-06},
	journal = {International Journal of Molecular Sciences},
	author = {Correale, Pierpaolo and Mutti, Luciano and Pentimalli, Francesca and Baglio, Giovanni and Saladino, Rita Emilena and Sileri, Pierpaolo and Giordano, Antonio},
	month = jul,
	year = {2020},
	pages = {5205},
	file = {Correale et al. - 2020 - HLA-B44 and C01 Prevalence Correlates with Covid.pdf:/home/biowitcher/snap/zotero-snap/common/Zotero/storage/F7EHLD5L/Correale et al. - 2020 - HLA-B44 and C01 Prevalence Correlates with Covid.pdf:application/pdf},
}

@article{pretti_class_2020,
	title = {Class {I} {HLA} {Allele} {Predicted} {Restricted} {Antigenic} {Coverages} for {Spike} and {Nucleocapsid} {Proteins} {Are} {Associated} {With} {Deaths} {Related} to {COVID}-19},
	volume = {11},
	issn = {1664-3224},
	url = {https://www.frontiersin.org/articles/10.3389/fimmu.2020.565730/full},
	doi = {10.3389/fimmu.2020.565730},
	abstract = {The world is dealing with one of the worst pandemics ever. SARS-CoV-2 is the etiological agent of COVID-19 that has already spread to more than 200 countries. However, infectivity, severity, and mortality rates do not affect all countries equally. Here we consider 140 HLA alleles and extensively investigate the landscape of 3,723 potential HLA-I A and B restricted SARS-CoV-2-derived antigens and how 37 countries in the world are predicted to respond to those peptides considering their HLA-I distribution frequencies. The clustering of HLA-A and HLA-B allele frequencies partially separates most countries with the lowest number of deaths per million inhabitants from the other countries. We further correlated the patterns of
              in silico
              predicted population coverage and epidemiological data. The number of deaths per million inhabitants correlates to the predicted antigen coverage of S and N derived peptides and its module is influenced if a given set of frequent or rare HLA alleles are analyzed in a given population. Moreover, we highlighted a potential risk group carrying HLAs associated with an elevated number of deaths per million inhabitants. In addition, we identified three potential antigens bearing at least one amino acid of the four-length insertion that differentiates SARS-CoV-2 from previous coronavirus strains. We believe these data can contribute to the search for peptides with the potential to be used in vaccine strategies considering the role of herd immunity to hamper the spread of the disease. Importantly, to the best of our knowledge, this work is the first to use a populational approach in association with COVID-19 outcome.},
	language = {en},
	urldate = {2022-06-06},
	journal = {Frontiers in Immunology},
	author = {Pretti, Marco Antônio M. and Galvani, Rômulo G. and Vieira, Gustavo Fioravanti and Bonomo, Adriana and Bonamino, Martín H. and Boroni, Mariana},
	month = dec,
	year = {2020},
	pages = {565730},
	file = {Pretti et al. - 2020 - Class I HLA Allele Predicted Restricted Antigenic .pdf:/home/biowitcher/snap/zotero-snap/common/Zotero/storage/GIPZR7NK/Pretti et al. - 2020 - Class I HLA Allele Predicted Restricted Antigenic .pdf:application/pdf},
}

