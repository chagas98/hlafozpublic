@article{bauer_evaluation_2016,
	title = {Evaluation of computational programs to predict {HLA} genotypes from genomic sequencing data},
	issn = {1467-5463, 1477-4054},
	url = {https://academic.oup.com/bib/article-lookup/doi/10.1093/bib/bbw097},
	doi = {10.1093/bib/bbw097},
	abstract = {Motivation: Despite being essential for numerous clinical and research applications, high-resolution human leukocyte antigen (HLA) typing remains challenging and laboratory tests are also time-consuming and labour intensive. With next-generation sequencing data becoming widely accessible, on-demand in silico HLA typing offers an economical and efﬁcient alternative. Results: In this study we evaluate the HLA typing accuracy and efﬁciency of ﬁve computational HLA typing methods by comparing their predictions against a curated set of {\textgreater} 1000 published polymerase chain reaction-derived HLA genotypes on three different data sets (whole genome sequencing, whole exome sequencing and transcriptomic sequencing data). The highest accuracy at clinically relevant resolution (four digits) we observe is 81\% on RNAseq data by PHLAT and 99\% accuracy by OPTITYPE when limited to Class I genes only. We also observed variability between the tools for resource consumption, with runtime ranging from an average of 5 h (HLAMINER) to 7 min (SEQ2HLA) and memory from 12.8 GB (HLA-VBSEQ) to 0.46 GB (HLAMINER) per sample. While a minimal coverage is required, other factors also determine prediction accuracy and the results between tools do not correlate well. Therefore, by combining tools, there is the potential to develop a highly accurate ensemble method that is able to deliver fast, economical HLA typing from existing sequencing data.},
	language = {en},
	urldate = {2022-06-06},
	journal = {Briefings in Bioinformatics},
	author = {Bauer, Denis C. and Zadoorian, Armella and Wilson, Laurence O. W. and {Melbourne Genomics Health Alliance} and Thorne, Natalie P.},
	month = nov,
	year = {2016},
	pages = {bbw097},
	file = {Bauer et al. - 2016 - Evaluation of computational programs to predict HL.pdf:/home/schagas/snap/zotero-snap/common/Zotero/storage/DLZLMS55/Bauer et al. - 2016 - Evaluation of computational programs to predict HL.pdf:application/pdf},
}

