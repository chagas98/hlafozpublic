	pages = {1792},
	file = {Guerini et al. - 2022 - HLA Allele Frequencies and Association with Severi.pdf:/home/schagas/snap/zotero-snap/common/Zotero/storage/5BE8JR3Q/Guerini et al. - 2022 - HLA Allele Frequencies and Association with Severi.pdf:application/pdf},
}

