@article{tavasolian_hla_2021,
	title = {{HLA}, {Immune} {Response}, and {Susceptibility} to {COVID}-19},
	volume = {11},
	issn = {1664-3224},
	url = {https://www.frontiersin.org/articles/10.3389/fimmu.2020.601886/full},
	doi = {10.3389/fimmu.2020.601886},
	abstract = {The severe acute respiratory syndrome caused by Coronavirus 2 (SARS-CoV-2) that appeared in December 2019 has precipitated the global pandemic Coronavirus Disease 2019 (COVID-19). However, in many parts of Africa fewer than expected cases of COVID-19, with lower rates of mortality, have been reported. Individual human leukocyte antigen (HLA) alleles can affect both the susceptibility and the severity of viral infections. In the case of COVID-19 such an analysis may contribute to identifying individuals at higher risk of the disease and the epidemiological level to understanding the differences between countries in the epidemic patterns. It is also recognized that first antigen exposure influences the consequence of subsequent exposure. We thus propose a theory incorporating HLA antigens, the “original antigenic sin (OAS)” effect, and presentation of viral peptides which could explain with differential susceptibility or resistance to SARS-CoV-2 infections.},
	language = {en},
	urldate = {2022-06-06},
	journal = {Frontiers in Immunology},
	author = {Tavasolian, Fataneh and Rashidi, Mohsen and Hatam, Gholam Reza and Jeddi, Marjan and Hosseini, Ahmad Zavaran and Mosawi, Sayed Hussain and Abdollahi, Elham and Inman, Robert D.},
	month = jan,
	year = {2021},
	pages = {601886},
	file = {Tavasolian et al. - 2021 - HLA, Immune Response, and Susceptibility to COVID-.pdf:/home/biowitcher/snap/zotero-snap/common/Zotero/storage/Y4Y95UK6/Tavasolian et al. - 2021 - HLA, Immune Response, and Susceptibility to COVID-.pdf:application/pdf},
}

