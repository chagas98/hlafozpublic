}

@article{mobini_kesheh_bioinformatic_2021,
	title = {Bioinformatic {HLA} {Studies} in the {Context} of {SARS}-{CoV}-2 {Pandemic} and {Review} on {Association} of {HLA} {Alleles} with {Preexisting} {Medical} {Conditions}},
